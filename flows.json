    {
        "id": "sequencer_function",
        "type": "function",
        "z": "d445d260.887a2",
        "name": "Antenna Sequencer",
        "func": "// msg.payload is t14/t29/t44/t59/force\nconst tick = msg.payload;\n\nconst txMode = (flow.get('txMode') || 'even').toString();\nconst txA = (flow.get('txAntenna') || 'deltaloop').toString();\nconst rxA = (flow.get('rxAntenna') || 'deltaloop').toString();\nlet action = null;\nconst out = [];\n\nif (tick === 'force'){\n    out.push({payload:true, topic: txA});\n    out.push({payload:true, topic: rxA});\n    return out;\n}\n\nif (txMode === 'even') {\n    if (tick === 't59' || tick === 't29') action = 'toTX';\n    else if (tick === 't14' || tick === 't44') action = 'toRX';\n} else if (txMode === 'odd'){\n    if (tick === 't14' || tick === 't44') action = 'toTX';\n    else if (tick === 't29' || tick === 't59') action = 'toRX';\n}\n\nif (action === 'toTX') {\n    out.push({payload:true, topic: txA});\n} else if (action === 'toRX') {\n    \n    out.push({payload:true, topic: rxA});\n}\nreturn out;",
        "outputs": 1,
        "timeout": "",
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 720,
        "y": 300,
        "wires": [
            [
                "invert_vertical",
                "invert_fandipole",
                "invert_deltaloop"
            ]
        ]
    },
